{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Javascript Vue",
	"injectionSelector": "L:string.template.js",
	"patterns": [
		
		{
			"include": "#vue-tag"
		}
	],
	"repository": {
		"vue-tag":{
			"name": "source.vue",
			"patterns": [
				{
					"include": "text.html.vue-html"
				}
			]
		}
	},
	"scopeName": "vuesource.js"
}