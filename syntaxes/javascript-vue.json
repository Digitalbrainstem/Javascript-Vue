{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Javascript Vue",
	"injectionSelector": "L:source.js",
	"patterns": [
		
		{
			"include": "#vue-tag1"
		}

	],
	"repository": {
		"vue-tag1":{
			"name":"vue-html",
			"contentName":"vue-html",
			"begin": "(template:)\\s*`",
			"beginCaptures": {
				"1":{
					"name":"vue-template"
				}
			},
			"end": "`",
			"patterns": [
				{
					"include": "text.html.vue-html"
				}
			]
		}
	},
	"scopeName": "javascript.vue.codeblock"
}